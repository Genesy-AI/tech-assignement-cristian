# Provider API Configuration
# Copy this file to .env and fill in your actual API keys

# Orion Connect Provider
# Best data quality but slower and can fail sometimes
ORION_CONNECT_AUTH_KEY=your_orion_connect_auth_key_here
ORION_CONNECT_BASE_URL=https://api.genesy.ai/api/tmp/orionConnect

# Nimbus Lookup Provider
# New provider in the market
NIMBUS_LOOKUP_API_KEY=your_nimbus_lookup_api_key_here
NIMBUS_LOOKUP_BASE_URL=https://api.genesy.ai/api/tmp/numbusLookup

# Astra Dialer Provider
# Fastest provider but with worst data quality
ASTRA_DIALER_API_KEY=your_astra_dialer_api_key_here
ASTRA_DIALER_BASE_URL=https://api.genesy.ai/api/tmp/astraDialer

# Database Configuration
DATABASE_URL="file:./dev.db"

# Temporal Configuration
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_NAMESPACE=default
TEMPORAL_TASK_QUEUE=myQueue
